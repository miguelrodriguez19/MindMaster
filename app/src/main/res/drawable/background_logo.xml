<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/blue_light_200" />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <gradient
                android:centerX="80%"
                android:centerY="15%"
                android:endColor="#00F57F17"
                android:gradientRadius="350dp"
                android:startColor="@color/blue_ribbon_500"
                android:type="radial" />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <gradient
                android:centerX="20%"
                android:centerY="100%"
                android:endColor="#00F57F17"
                android:gradientRadius="400dp"
                android:startColor="@color/blue_san_juan_700"
                android:type="radial" />
        </shape>
    </item>

</layer-list>
